# Discord bot token (REQUIRED)
DISCORD_TOKEN=

# OpenWeather
OPENWEATHER_API_KEY=
# Cache weather forecast responses for N seconds (default 600)
OPENWEATHER_CACHE_TTL_SECONDS=600
# Request timeout seconds (default 10)
OPENWEATHER_TIMEOUT_SECONDS=10

# Routes sheet export URL (Google Sheets .xlsx export)
ROUTES_XLSX_URL=
# Cache parsed routes for N seconds (default 1800)
ROUTES_CACHE_TTL_SECONDS=1800
# Request timeout seconds (default 15)
ROUTES_TIMEOUT_SECONDS=15
# Optional disk cache path
ROUTES_CACHE_PATH=routes_cache.json

# Route master inputs (optional defaults)
# LAT=40.102121327005165
# LON=-88.22681926647813
